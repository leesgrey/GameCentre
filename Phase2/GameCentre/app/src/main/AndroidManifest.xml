<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="companydomain.applicationname">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="companydomain.applicationname.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="companydomain.applicationname.SignUpActivity"></activity>
        <activity android:name="companydomain.applicationname.SlidingTilesActivity"></activity>
        <activity android:name="companydomain.applicationname.StartingActivity"></activity>
        <activity android:name="companydomain.applicationname.ChooseGameActivity"></activity>
        <activity android:name="companydomain.applicationname.ScoreboardActivity"></activity>
    </application>

</manifest>